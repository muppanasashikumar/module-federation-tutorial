{
  "name": "share-module-federation-tutorial",
  "version": "0.1.5",
  "description": "Example to share theme and context using module federation",
  "private": false,
  "license": "MIT",
  "author": "victor santos",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc",
    "start": "node ./dist/index.js",
    "build": "npm run compile",
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' ./src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "CURRENT_ENV=test jest --setupFiles dotenv/config --config ./jest.config.js ./src",
    "test:watch": "CURRENT_ENV=test jest --setupFiles dotenv/config --watch --config ./jest.config.js ./src"
  },
  "peerDependencies": {
    "@mui/material": "^5.11.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/node-forge": "^1.3.1",
    "@types/supertest": "^2.0.12",
    "@types/xml-crypto": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
